<section id='#benefits' class="section text-parallax">
  <div class="container-xxl">
    <div class="row">
      <div class="col-lg-4 col-xl-6">
        <h3 class="tagline">{{ content.field_tagline }}</h3>
        <h4 class="headline">{{ content.field_heading }}</h4>
        <p class="summary">{{ content.field_summary }}</p>
        <div class="py-4 d-flex justify-content-center">
          <a data-anchor="#contact" class="cta mt-3">{{ content.field_link.0['#title'] }}</a>
        </div>
      </div>
      <div class="col-lg-8 col-xl-6">
      {% set graphicsUrl = '/sites/default/files/2023-12/vennyucon.png' %}
      {% set backgroundUrl = "background-image:url(" ~ file_url(paragraph.field_image.entity.field_media_image.entity.fileuri) ~ ")" %}
        <div class="parallax-wrapper" style={{ backgroundUrl }}>
          {% if graphicsUrl %}
            <div id="parallax"></div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</section>


  